    @model Dojo 
    <div class="container">
        <h1 class="text-center mt-5">Submitted Info</h1>
        <div class="row mt-5 d-flex flex-row justify-content-center">
            <div class="col-8 d-flex flex-column align-items-center border border-2 p-2">
                <div class="d-flex flex-row justify-content-between align-items-center gap-5 p-5">
                    <h3 class="fw-bold" >Name :</h3>
                    <h3>@Model.Name</h3>
                </div>
                <div class="d-flex flex-row justify-content-between align-items-center gap-5 p-5">
                    <h3 class="fw-bold" >Dojo Location :</h3>
                    <h3>@Model.Location</h3>
                </div>
                <div class="d-flex flex-row justify-content-between align-items-center gap-5 p-5">
                    <h3 class="fw-bold" >Favorite Language :</h3>
                    <h3>@Model.Language</h3>
                </div>
                <div class="d-flex flex-row justify-content-between align-items-center gap-5 p-5">
                    <h3 class="fw-bold" >Comments :</h3>
                    <h3>@Model.Comment</h3>
                </div>
            </div>
        </div>
    </div>